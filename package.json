{
  "name": "gulp-starter-react-cjsx",
  "version": "0.1.0",
  "description": "Gulp starter with common tasks and scenarios for ReactJS with cjsx (coffee + jsx support)",
  "repository": {
    "type": "git",
    "url": "https://github.com/piotrpalek/gulp-starter"
  },
  "browser": {
    "underscore": "backbone/node_modules/underscore"
  },
  "browserify": {
    "transform": [
      "browserify-shim",
      "coffee-reactify",
      "hbsfy"
    ],
    "extensions": [".cjsx"],
    "debug": true
  },
  "browserify-shim": {
    "jquery": "$",
    "underscore": "_"
  },
  "devDependencies": {
    "browserify-shim": "~3.4.1",
    "browserify": "~3.36.0",
    "coffee-reactify": "latest",
    "connect": "~2.14.3",
    "gulp-sass": "^0.7.1",
    "gulp-changed": "~0.3.0",
    "gulp-imagemin": "~0.1.5",
    "gulp-livereload": "~1.2.0",
    "gulp-notify": "~1.2.4",
    "gulp-open": "~0.2.8",
    "gulp": "~3.6.0",
    "hbsfy": "~1.3.2",
    "vinyl-source-stream": "~0.1.1",
    "jest-cli": "^0.1.13",
    "coffee-react": "latest"
  },
  "dependencies": {
    "react": "latest",
    "envify": "latest",
    "coffee-react": "latest",
    "underscore": "latest",
    "backbone": "latest"
  },
  "scripts": {
    "test": "jest"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/preprocessor.js",
    "testDirectoryName": "specs",
    "testFileExtensions": ["coffee", "js", "cjsx"],
    "unmockedModulePathPatterns": ["<rootDir>/node_modules/react"]
  }
}
